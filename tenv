#!/bin/bash -f
rm -f core.*
export PATH=.:$PATH
export GCIN_TABLE_DIR=./data
echo "GCIN_TABLE_DIR=$GCIN_TABLE_DIR"
export GCIN_IM_CLIENT_NO_AUTO_EXEC=
pkill gcin.test
unset LC_CTYPE
export LC_CTYPE=zh_TW.UTF-8
#export LANG=zh_TW
export XMODIFIERS=@im=gcin_test
#export GCIN_OUTPUT_FILTER=./gcin_filter
./gcin.test &
gcinpid=$!
sleep 1
export LD_PRELOAD=./im-client/libgcin-im-client.so
#unset GTK_IM_MODULE
gedit
#leafpad
pkill gcin.test
